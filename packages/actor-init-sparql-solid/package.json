{
  "name": "@comunica/actor-init-sparql-solid",
  "version": "1.0.0",
  "description": "A Comunica engine for query evaluation over Solid data pods",
  "lsd:module": true,
  "main": "index.js",
  "typings": "index",
  "repository": "https://github.com/comunica/comunica-feature-solid/tree/master/packages/actor-init-sparql-solid",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "actor",
    "init",
    "sparql",
    "solid"
  ],
  "license": "MIT",
  "bin": {
    "comunica-sparql-solid": "./bin/query.js",
    "comunica-sparql-solid-http": "./bin/http.js",
    "comunica-dynamic-sparql-solid": "./bin/query-dynamic.js"
  },
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "config",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.js",
    "index.d.ts",
    "index-browser.d.ts",
    "index-browser.js",
    "engine-default.js"
  ],
  "dependencies": {
    "@comunica/actor-init-sparql": "^1.22.0",
    "@comunica/actor-http-inrupt-solid-client-authn": "^1.0.0",
    "@comunica/context-entries": "^1.22.0",
    "@comunica/core": "^1.22.0",
    "@comunica/runner-cli": "^1.22.0",
    "@comunica/runner": "^1.22.0",
    "solid-node-interactive-auth": "^1.0.1",
    "@rubensworks/solid-client-authn-isomorphic": "^1.0.0"
  },
  "scripts": {
    "lint": "node \"../../node_modules/tslint/bin/tslint\" lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls",
    "prepare": "comunica-compile-config config/config-default.json > engine-default.js"
  }
}
